{
	"html document": {
		"prefix": "html",
		"body": [
			"doctype html",
			"",
			"html(lang=\"en\")",
			"\thead",
			"\t\tmeta(charset=\"UTF-8\")",
			"\t\tmeta(name=\"viewport\", content=\"width=device-width, initial-scale=1.0\")",
			"\t\tmeta(http-equiv=\"X-UA-Compatible\", content=\"ie=edge\")",
			"\t\ttitle ${1:Document}",
			"\tbody",
			"\t\t$2"
