{
	"name": "code-d",
	"displayName": "D Programming Language (code-d)",
	"description": "auto-complete, snippets, linter and formatter for dlang",
	"version": "0.15.1",
	"publisher": "webfreak",
	"repository": {
		"type": "git",
		"url": "https://github.com/Pure-D/code-d.git"
	},
	"icon": "images/dlogo-square.svg",
	"license": "MIT",
	"engines": {
		"vscode": "^1.6.0"
	},
	"categories": [
		"Languages",
		"Snippets",
		"Linters",
		"Formatters",
		"Other"
	],
	"activationEvents": [
		"onLanguage:d",
		"onLanguage:dml",
		"workspaceContains:dub.json",
		"workspaceContains:dub.sdl",
		"onCommand:code-d.switchConfiguration",
		"onCommand:code-d.switchArchType",
		"onCommand:code-d.switchBuildType",
		"onCommand:code-d.switchCompiler",
		"onCommand:code-d.killServer",
		"onCommand:code-d.restartServer",
		"onCommand:code-d.reloadImports",
		"onCommand:code-d.run",
		"onCommand:code-d.build",
		"onCommand:code-d.stop",
		"onCommand:code-d.debug",
		"onCommand:code-d.uploadSelection",
		"onCommand:code-d.insertDscanner",
		"onCommand:code-d.rdmdCurrent",
		"onCommand:code-d.generateCoverageReport",
		"onCommand:code-d.showGCCalls",
		"onCommand:dub.openSettingsEditor",
		"onCommand:dub.closeSettingsEditor",
		"onCommand:code-d.createProject",
		"onCommand:code-d.addImport"
	],
	"main": "./out/src/extension",
	"extensionDependencies": [
		"vscode.jade"
	],
	"contributes": {
		"languages": [
			{
				"id": "d",
				"extensions": [
					".d",
					".di"
				],
				"aliases": [
					"D"
				],
				"configuration": "./lang-configs/d.json"
			},
			{
				"id": "dscript",
				"aliases": [
					"arsd.DScript"
				],
				"extensions": [
					".dscript",
					".ds"
				],
				"configuration": "./lang-configs/d.json"
			},
			{
				"id": "dml",
				"aliases": [
					"DlangUI Markup Language"
				],
				"extensions": [
					".dml"
				],
				"configuration": "./lang-configs/d.json"
			},
			{
				"id": "sdl",
				"aliases": [
					"Simple Declarative Language"
				],
				"extensions": [
					".sdl"
				],
				"configuration": "./lang-configs/d.json"
			},
			{
				"id": "diet",
				"aliases": [
					"Diet"
				],
				"extensions": [
					".dt"
				]
			}
		],
		"grammars": [
			{
				"language": "d",
				"scopeName": "source.d",
				"path": "./syntaxes/d.tmLanguage"
			},
			{
				"language": "dscript",
				"scopeName": "source.dscript",
				"path": "./syntaxes/arsd.dscript.tmLanguage"
			},
			{
				"language": "dml",
				"scopeName": "source.dml",
				"path": "./syntaxes/dml.json"
			},
			{
				"language": "sdl",
				"scopeName": "source.sdl",
				"path": "./syntaxes/sdl.json"
			},
			{
				"language": "diet",
				"scopeName": "source.diet",
				"path": "./syntaxes/diet.json"
			}
		],
		"snippets": [
			{
				"language": "d",
				"path": "./snippets/d.json"
			}
		],
		"configuration": {
			"title": "D configuration",
			"type": "object",
			"properties": {
				"d.workspacedPath": {
					"type": "string",
					"default": "workspace-d",
					"description": "%d.config.workspacedPath%"
				},
				"d.stdlibPath": {
					"type": "array",
					"items": {
						"type": "string"
					},
					"default": [
						"/usr/include/dmd/druntime/import",
						"/usr/include/dmd/phobos"
					],
					"description": "%d.config.stdlibPath%"
				},
				"d.dcdClientPath": {
					"type": "string",
					"default": "dcd-client",
					"description": "%d.config.dcdClientPath%"
				},
				"d.dcdServerPath": {
					"type": "string",
					"default": "dcd-server",
					"description": "%d.config.dcdServerPath%"
				},
				"d.dscannerPath": {
					"type": "string",
					"default": "dscanner",
					"description": "%d.config.dscannerPath%"
				},
				"d.dfmtPath": {
					"type": "string",
					"default": "dfmt",
					"description": "%d.config.dfmtPath%"
				},
				"d.dubPath": {
					"type": "string",
					"default": "dub",
					"description": "%d.config.dubPath%"
				},
				"d.enableLinting": {
					"type": "boolean",
					"default": true,
					"description": "%d.config.enableLinting%"
				},
				"d.enableSDLLinting": {
					"type": "boolean",
					"default": true,
					"description": "%d.config.enableSDLLinting%"
				},
				"d.enableDubLinting": {
					"type": "boolean",
					"default": true,
					"description": "%d.config.enableDubLinting%"
				},
				"d.enableAutoComplete": {
					"type": "boolean",
					"default": true,
					"description": "%d.config.enableAutoComplete%"
				},
				"d.neverUseDub": {
					"type": "boolean",
					"default": false,
					"description": "%d.config.neverUseDub%"
				},
				"d.projectImportPaths": {
					"type": "array",
					"items": {
						"type": "string"
					},
					"default": [],
					"description": "%d.config.projectImportPaths%"
				},
				"d.dubConfiguration": {
					"type": "string",
					"description": "%d.config.dubConfiguration%"
				},
				"d.dubArchType": {
					"type": "string",
					"description": "%d.config.dubArchType%"
				},
				"d.dubBuildType": {
					"type": "string",
					"description": "%d.config.dubBuildType%"
				},
				"d.dubCompiler": {
					"type": "string",
					"description": "%d.config.dubCompiler%"
				},
				"d.disableWorkspaceD": {
					"type": "boolean",
					"default": false,
					"description": "%d.config.disableWorkspaceD%"
				},
				"d.overrideDfmtEditorconfig": {
					"type": "boolean",
					"default": true,
					"description": "%d.config.overrideDfmtEditorconfig%"
				},
				"dfmt.alignSwitchStatements": {
					"type": "boolean",
					"default": true,
					"description": "%d.config.dfmt.alignSwitchStatements%"
				},
				"dfmt.braceStyle": {
					"type": "string",
					"enum": [
						"allman",
						"otbs",
						"stroustrup"
					],
					"default": "allman",
					"description": "%d.config.dfmt.braceStyle%"
				},
				"dfmt.outdentAttributes": {
					"type": "boolean",
					"default": true,
					"description": "%d.config.dfmt.outdentAttributes%"
				},
				"dfmt.spaceAfterCast": {
					"type": "boolean",
					"default": true,
					"description": "%d.config.dfmt.spaceAfterCast%"
				},
				"dfmt.splitOperatorAtLineEnd": {
					"type": "boolean",
					"default": false,
					"description": "%d.config.dfmt.splitOperatorAtLineEnd%"
				},
				"dfmt.selectiveImportSpace": {
					"type": "boolean",
					"default": true,
					"description": "%d.config.dfmt.selectiveImportSpace%"
				},
				"dfmt.compactLabeledStatements": {
					"type": "boolean",
					"default": true,
					"description": "%d.config.dfmt.compactLabeledStatements%"
				},
				"dfmt.templateConstraintStyle": {
					"type": "string",
					"enum": [
						"conditional_newline_indent",
						"conditional_newline",
						"always_newline",
						"always_newline_indent"
					],
					"default": "conditional_newline_indent",
					"description": "%d.config.dfmt.templateConstraintStyle%"
				}
			}
		},
		"menus": {
			"explorer/context": [
				{
					"when": "resourceLangId == d",
					"command": "code-d.rdmdCurrent",
					"group": "1_modification"
				}
			],
			"editor/title": [
				{
					"when": "resourceFilename == 'dub.json'",
					"command": "dub.openSettingsEditor",
					"group": "navigation"
				},
				{
					"when": "resourceScheme == dubsettings",
					"command": "dub.closeSettingsEditor",
					"group": "navigation"
				}
			]
		},
		"commands": [
			{
				"command": "code-d.switchConfiguration",
				"title": "%d.switchConfiguration%"
			},
			{
				"command": "code-d.switchArchType",
				"title": "%d.switchArchType%"
			},
			{
				"command": "code-d.switchBuildType",
				"title": "%d.switchBuildType%"
			},
			{
				"command": "code-d.switchCompiler",
				"title": "%d.switchCompiler%"
			},
			{
				"command": "code-d.killServer",
				"title": "%d.killServer%"
			},
			{
				"command": "code-d.restartServer",
				"title": "%d.restartServer%"
			},
			{
				"command": "code-d.reloadImports",
				"title": "%d.reloadImports%"
			},
			{
				"command": "code-d.run",
				"title": "%d.run%"
			},
			{
				"command": "code-d.build",
				"title": "%d.build%"
			},
			{
				"command": "code-d.stop",
				"title": "%d.stop%"
			},
			{
				"command": "code-d.debug",
				"title": "%d.debug%"
			},
			{
				"command": "code-d.uploadSelection",
				"title": "%d.uploadSelection%"
			},
			{
				"command": "code-d.insertDscanner",
				"title": "%d.insertDscanner%"
			},
			{
				"command": "code-d.rdmdCurrent",
				"title": "%d.rdmdCurrent%"
			},
			{
				"command": "code-d.generateCoverageReport",
				"title": "%d.generateCoverageReport%"
			},
			{
				"command": "code-d.showGCCalls",
				"title": "%d.showGCCalls%"
			},
			{
				"command": "dub.openSettingsEditor",
				"title": "%dub.openSettingsEditor%",
				"icon": {
					"light": "./images/edit-dub-inverse.svg",
					"dark": "./images/edit-dub.svg"
				}
			},
			{
				"command": "dub.closeSettingsEditor",
				"title": "%dub.closeSettingsEditor%",
				"icon": {
					"light": "./images/edit-source-inverse.svg",
					"dark": "./images/edit-source.svg"
				}
			},
			{
				"command": "code-d.createProject",
				"title": "%d.createProject%"
			}
		],
		"jsonValidation": [
			{
				"fileMatch": "dub.json",
				"url": "./json-validation/dub.schema.json"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "tsc -p ./",
		"compile": "tsc -watch -p ./",
		"postinstall": "node ./node_modules/vscode/bin/install"
	},
	"dependencies": {
		"request": "^2.76.0",
		"request-progress": "^2.0.1",
		"jsonc-parser": "^0.2.3",
		"unzip": "^0.1.11",
		"async": "^2.1.2",
		"ncp": "^2.0.0",
		"rmdir": "^1.2.0",
		"line-by-line": "^0.1.5",
		"vscode-nls": "WebFreak001/vscode-nls#9954e54"
	},
	"devDependencies": {
		"typescript": "^2.0.6",
		"vscode": "^1.0.1",
		"mocha": "^3.1.2",
		"@types/node": "^6.0.40",
		"@types/mocha": "^2.2.32"
	}
}