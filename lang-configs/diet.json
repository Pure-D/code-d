{
	"comments": {
		"lineComment": "//"
	},
	"brackets": [
		[
			"(",
			")"
		],
		[
			"{",
			"}"
		],
		[
			"[",
			"]"
		]
	],
	"autoClosingPairs": [
		[
			"{",
			"}"
		],
		[
			"[",
			"]"
		],
		[
			"(",
			")"
		],
		{
			"open": "<",
			"close": ">",
			"notIn": [
				"embedded",
				"string"
			]
		},
		{
			"open": "`",
			"close": "`",
			"notIn": [
				"string"
			]
		},
		{
			"open": "\"",
			"close": "\"",
			"notIn": [
				"string"
			]
		},
		{
			"open": "'",
			"close": "'",
			"notIn": [
				"string"
			]
		}
	],
	"surroundingPairs": [
		[
			"{",
			"}"
		],
		[
			"[",
			"]"
		],
		[
			"(",
			")"
		],
		[
			"'",
			"'"
		],
		[
			"\"",
			"\""
		],
		[
			"`",
			"`"
		]
	],
	"folding": {
		"offSide": true
	},
	"wordPattern": "[^\\`\\~\\!\\@\\#\\%\\^\\&\\*\\(\\)\\=\\+\\[\\{\\]\\}\\\\\\|\\;\\:\\'\\\"\\,\\.\\<\\>\\/\\?\\s]+"
}